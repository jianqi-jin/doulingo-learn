{
  "manifest_version": 2,
  "name": "Googleplay Spider",
  "description": "Base Level Extension",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["https://*.play.google.com/*"],
      "js": ["./content.js"]
    }
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": "./static/exten-icon.png"
  }
}
